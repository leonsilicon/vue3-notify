		"eslint-define-config": "^1.4.0",
		"execa": "^6.1.0",
		"happy-dom": "^2.55.0",
		"lion-system": "^1.3.20",
		"typescript": "^4.6.3",
		"vite": "^2.9.5",
		"vite-plugin-dts": "^1.1.1",
