{
	"name": "vue3-notify",
	"version": "0.0.0",
	"description": "Vue 3 Notification library",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/leonzalion/vue3-notify.git"
	},
	"homepage": "https://github.com/leonzalion/vue3-notify#readme",
	"author": {
		"name": "Leon Si",
		"email": "contact@leonzalion.com",
